    "bootstrap": "^5.3.3",
    "react-bootstrap": "^2.10.4",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "wait-on": "^8.0.1"
  }
}
